{
  "name": "bora-token-klaytn",
  "version": "1.0.0",
  "description": "Truffle framework for testing BORA Token",
  "main": "truffle-config.js",
  "scripts": {
    "test": "truffle compile && (npm run testrpc > /dev/null & truffle test) && kill -9 $(lsof -ti :8545)",
    "testrpc": "npx ganache-cli -p 8545",
    "console": "truffle console",
    "flattener": "truffle-flattener contracts/BoraToken.sol --output dist/BoraToken.dist.sol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boraecosystem/bora-token-klaytn.git"
  },
  "author": "boraecosystem <contact@boraecosystem.com>",
  "license": "MIT",
  "homepage": "https://www.boraecosystem.com",
  "dependencies": {
    "openzeppelin-solidity": "2.3.0"
  },
  "devDependencies": {
    "ganache-cli": "^6.7.0",
    "truffle": "5.0.26",
    "truffle-flattener": "^1.4.2"
  }
}
